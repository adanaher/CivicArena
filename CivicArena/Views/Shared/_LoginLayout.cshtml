<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Civic Arena</title>
    @Styles.Render("~/Content/css/bootstrap.min.css")
    @Styles.Render("~/Content/css/bootstrap-slider.css")
    @Styles.Render("~/Content/css/style.css")
    <style>
        .logo {
            width: 50%;
            height: 20%;
            display: inline-block;
            overflow: hidden;
        }
    </style>
    <script src="https://apis.google.com/js/platform.js" async defer></script>
</head>
<body bgcolor="#114150">
    <div id="fb-root"></div>
    <script>
(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.5&appId=297276526958905";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
    <script>
      function onSignIn(googleUser) {
        // Useful data for your client-side scripts:
        var profile = googleUser.getBasicProfile();
        console.log("ID: " + profile.getId()); // Don't send this directly to your server!
        console.log('Full Name: ' + profile.getName());
        console.log('Given Name: ' + profile.getGivenName());
        console.log('Family Name: ' + profile.getFamilyName());
        console.log("Image URL: " + profile.getImageUrl());
        console.log("Email: " + profile.getEmail());

        // The ID token you need to pass to your backend:
        var id_token = googleUser.getAuthResponse().id_token;
        console.log("ID Token: " + id_token);
      };
    </script>

    <center>

        @RenderBody()

    </center>
        @Scripts.Render("~/Scripts/js/jquery.js")
        @Scripts.Render("~/Scripts/js/bootstrap.min.js")
        @Scripts.Render("~/Scripts/js/jquery.easing.min.js")
        @Scripts.Render("~/Scripts/js/jquery.fittext.js")
        @Scripts.Render("~/Scripts/js/wow.min.js")
        @Scripts.Render("~/Scripts/js/creative.js")
        @Scripts.Render("~/Scripts/js/modernizr.js")
        @RenderSection("scripts", required: false)
</body>
</html>
